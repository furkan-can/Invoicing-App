{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }
      ],
      "@babel/react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread",
      [
        "module-resolver",
        {
          "alias": {
            "$c": "./src/components",
            "$p": "./src/pages",
            "$u": "./src/utils"
          }
        }
      ]
    ]
  }